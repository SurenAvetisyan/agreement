<odoo>

    <record id="agreement_types_tree" model="ir.ui.view">
        <field name="name">Agreement Types tree</field>
        <field name="model">agreement.types</field>
        <field name="arch" type="xml">
            <tree string="Agreement Types">
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="agreement_types_form" model="ir.ui.view">
        <field name="name">Agreement Types form</field>
        <field name="model">agreement.types</field>
        <field name="arch" type="xml">
            <form string="Agreement Types">
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="active" />
                    </group>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids"/>
                    <field name="activity_ids"/>
                    <field name="message_ids" />
                </div>
            </form>
        </field>
    </record>

    <record id="agreement_types_action" model="ir.actions.act_window">
        <field name="name">Agreement Types</field>
        <field name="res_model">agreement.types</field>
        <field name="view_mode">tree,form</field>
    </record>


</odoo>